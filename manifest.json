{
  "name": "Custodian",
  "version": "0.0.1",
  "description": "Replace words across the web",
  "manifest_version": 3,
  "background":{
    "service_worker": "serviceWorker.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon":"logo.png"
  },
  "host_permissions": ["<all_urls>"],
  "permissions": ["scripting", "storage","tabs"],
  "options_ui":{
    "page": "opt.html"
  }
}
